    "nuka-carousel": "^4.3.5",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-async-script-loader": "^0.3.0",
    "react-device": "^0.8.1",
    "react-dom": "^16.4.1",
    "react-dropzone": "^5.0.1",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.4",
    "react-image-crop": "^4.0.3",
    "react-infinite-scroller": "^1.2.0",
    "react-modal": "^3.5.1",
    "react-redux": "^5.0.7",
