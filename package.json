  "scripts": {
    "start": "react-scripts start",
    "now-start": "serve --single ./build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive --usePolling --polling-interval 500",
    "deploy:dev": "./node_modules/.bin/now -t ${NOW_TOKEN} -e STRIPE_TOKEN=\"${STRIPE_TOKEN}\" -e PAYPAL_CLIENT=\"${PAYPAL_CLIENT}\" -e PAYPAL_SECRET=\"${PAYPAL_SECRET}\"",
    "deploy:prod": "./node_modules/.bin/now -t ${NOW_TOKEN} -e STRIPE_TOKEN=\"${PROD_STRIPE_TOKEN}\" -e PAYPAL_CLIENT=\"${PROD_PAYPAL_CLIENT}\" -e PAYPAL_SECRET=\"${PROD_PAYPAL_SECRET}\"",
    "deploy:alias-now-dev": "./node_modules/.bin/now -t ${NOW_TOKEN} alias hmr-dev",
    "deploy:alias-now-qa": "./node_modules/.bin/now -t ${NOW_TOKEN} alias hmr-qa",
    "deploy:alias-now-stage": "./node_modules/.bin/now -t ${NOW_TOKEN} alias hmr-stage",
    "deploy:alias-now-prod": "./node_modules/.bin/now -t ${NOW_TOKEN} alias hearmyroar.org",
    "deploy:alias-now-prod-www": "./node_modules/.bin/now -t ${NOW_TOKEN} alias www.hearmyroar.org",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint ./src",
    "lint:fix": "npm run lint --fix"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^3.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "now": "^11.3.9",
    "react-app-env": "^1.2.2",
    "react-styleguidist": "^7.2.5",
    "redux-logger": "^3.0.6"
  }
}
